AllowAgentForwarding no
#AllowStreamLocalForwarding no
AllowTcpForwarding no
AllowUsers @VMCONSOLE_USER@
AuthorizedKeysCommand @libexecdir_POST@/ovirt-vmconsole-proxy-keys
AuthorizedKeysCommandUser @VMCONSOLE_USER@
ChallengeResponseAuthentication no
ClientAliveCountMax 3
ClientAliveInterval 10
GSSAPIAuthentication no
HostCertificate @pkgpkidir_POST@/proxy-ssh_host_rsa-cert.pub
HostKey @pkgpkidir_POST@/proxy-ssh_host_rsa
HostbasedAuthentication no
KbdInteractiveAuthentication no
KerberosAuthentication no
PasswordAuthentication no
#PermitUserRC no
PidFile /dev/null
Port @OVIRT_VMCONSOLE_PROXY_SSHD_PORT@
Protocol 2
PubkeyAuthentication yes
TrustedUserCAKeys @pkgpkidir_POST@/ca.pub
X11Forwarding no
